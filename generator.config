[generator]
input_dir   = "./TCIA_SegPC_dataset"
;dataset = ["test", "train", "valid"]
datasets = ["train", "valid"]
width      = 640
height     = 640
output_dir = "./Augmented-MultipleMyeloma-ImageMask-Dataset_V5"
mask_merge = True
crop    = False
augment = True

[augmentor]
flip   = True
mirror = True
angles = [90, 180, 270]

